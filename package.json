{
    "name": "cli-config",
    "version": "0.3.2",
    "description": "Dead simple 'one call' API to convert your lib into a command line interface. Merges system & user settings, CLI options & actions, app overrides and package.json into one config object. Optionally selects a command exported by your libs and executes it. BYO YAML/CoffeeScript or any other parser to support non-JSON file formats.",
    "homepage": "https://github.com/tohagan/cli-config",
    "author": {
        "name": "Tony O'Hagan",
        "email": "tony@ohagan.name"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:tohagan/cli-config.git"
    },
    "keywords": [
        "config",
        "configuration",
        "settings",
        "options",
        "cli",
        "yaml",
        "coffee",
        "coffeescript",
        "command-line",
        "default",
        "defaults",
        "json",
        "minimist"
    ],
    "preferGlobal": "false",
    "main": "lib/index.js",
    "scripts": {
        "test": "jasmine-node --color spec"
    },
    "engines": {
        "node": ">=0.10.0"
    },
    "dependencies": {
        "strip-json-comments": "0.1.3",
        "minimist": "0.2.0"
    }, 
    "devDependencies": {
        "jasmine-node": "1.8.0"
    }
}
